{
  "name": "propertyme-coding-challenge",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "make-sure-npm-install-happened-before": "[ ! -d 'node_modules/@nrwl' ] && npm install || true",
    "vercel-build": "npx nx build --prod --verbose",
    "start": "nx start demo",
    "prebuild": "npm run make-sure-npm-install-happened-before",
    "build": "nx build demo",
    "test": "run-p test:*",
    "test:demo": "nx test demo",
    "test:ui-link": "nx test ui-link",
    "test:ui-link-button": "nx test ui-link-button",
    "typecheck": "run-p typecheck:*",
    "typecheck:demo": "npx nx tsc demo",
    "typecheck:ui-link": "npx nx tsc ui-link",
    "typecheck:ui-link-button": "npx nx tsc ui-link-button",
    "spellcheck": "cspell '**/*' --unique",
    "spellcheck:ci": "cspell '**/*' --unique --fail-fast",
    "spellcheck:add-unknown-words-to-custom-dictionary": "cspell '**/*' --unique --words-only --no-progress --no-summary >> .cspell/custom-words.txt && cat .cspell/custom-words.txt",
    "predev": "npm run make-sure-npm-install-happened-before",
    "dev": "run-p dev:open dev:start",
    "dev:start": "nx dev demo",
    "dev:open": "wait-on http://localhost:3000 && open-cli http://localhost:3000",
    "postinstall": "npx remix setup node"
  },
  "private": true,
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@nrwl/remix": "14.7.0",
    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/serve": "^1.7.4",
    "@remix-run/vercel": "^1.7.4",
    "@types/styled-components": "^5.1.26",
    "core-js": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@lukalabs/esbuild-inject-plugin": "^1.0.2",
    "@nrwl/cli": "15.0.4",
    "@nrwl/jest": "14.8.6",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.8.6",
    "@nrwl/web": "14.8.6",
    "@nrwl/workspace": "15.0.4",
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.6",
    "babel-jest": "28.1.1",
    "cspell": "^6.14.1",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "npm-run-all": "^4.1.5",
    "nx": "15.0.4",
    "open-cli": "^7.1.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "workspaces": [
    "libs/*"
  ]
}
