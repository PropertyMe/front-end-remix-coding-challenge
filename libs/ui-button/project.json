{
  "name": "ui-button",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui-button/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "libs/ui-button/dist",
        "tsConfig": "libs/ui-button/tsconfig.lib.json",
        "project": "libs/ui-button/package.json",
        "entryFile": "libs/ui-button/src/index.ts",
        "external": [
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/ui-button/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "format": [
          "cjs"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/ui-button/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/ui-button"
      ],
      "options": {
        "jestConfig": "libs/ui-button/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
